{% extends 'baseView.twig' %}
{# eventually, baseView will be restructured to include the header/h1, this is to build in future compatibility #}
{% set _content_header = block("content_header") %}
{% block content %}
    {% if _content_header is empty %}
        <h1>{{ PageTitle }}</h1>
    {% endif %}
    {{ MainPost.content }}
{% endblock %}